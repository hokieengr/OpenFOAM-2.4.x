FoamFile
{
    version 2.0;
    format ascii;
    class volVectorField;
    object U;
}

// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //
dimensions [0 1 -1 0 0 0 0];  // m/s

// For angle of attack without sideslip
// x = Vinf * cos(alpha)
// y = 0; 
// z = Vinf * sin(alpha)
internalField uniform (200.0 0 0);

boundaryField
{
    inlet
    {
        type freestream;
        freestreamValue uniform (200.0 0 0);
    }
    
    outlet
    {
        type freestream;
        freestreamValue uniform (200.0 0 0);
    }
    
    front
    {
        type freestream;
        freestreamValue uniform (200.0 0 0);
    }
    
    top
    {
        type freestream;
        freestreamValue uniform (200.0 0 0);
    }
    
    back
    {
        type freestream;
        freestreamValue uniform (200.0 0 0);
    }
    
    bottom
    {
        type freestream;
        freestreamValue uniform (200.0 0 0);
    }
}
// ************************************************************************* //
